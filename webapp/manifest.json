{
  "_version": "1.76.0",
  "sap.app": {
    "id": "sample.project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "106f8e89-51fb-4d85-a411-45111ea7f406"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.uxap": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sample.project1.i18n.i18n"
        }
      },
      "i18n_demo": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sample.project1.i18n.i18n_demo"
        }
      },
      "orderDetailModel": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/orderData.json"
      },
      "employeeDetailModel": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/employeeData.json"
      },
      "departmentModel": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/Departments.json"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "sample.project1.view",
        "async": true,
        "viewPath": "sample.project1.view"
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": "",
          "target": [
            "TargetView1"
          ]
        },
        {
          "name": "RouteUser",
          "pattern": "user",
          "target": [
            "TargetUser"
          ]
        },
        {
          "name": "WelcomePage",
          "pattern": "welcome/{userName}",
          "target": [
            "WelcomeUser"
          ]
        },
        {
          "name": "EmployeePage",
          "pattern": "employee",
          "target": [
            "EmployeePage"
          ]
        }
      ],
      "targets": {
        "TargetView1": {
          "viewName": "View1",
          "viewId": "view1"
        },
        "TargetUser": {
          "viewName": "View2",
          "viewId": "view2"
        },
        "WelcomeUser": {
          "viewName": "View3",
          "viewId": "view3"
        },
        "EmployeePage": {
          "viewName": "Employee",
          "viewId": "emp"
        }
      }
    },
    "rootView": {
      "viewName": "sample.project1.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}